{
  "manifest_version": 2,
  "name": "Fetch Page Content",
  "version": "1.0",
  "description": "Fetch and print the HTML content of wsbnlu.clouda.edu.pl pages.",
  "permissions": [
    "activeTab",
    "tabs",
    "https://wsbnlu.clouda.edu.pl/*"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
