{
  "manifest_version": 2,
  "name": "WSB AI Test Solver",
  "version": "1.0",
  "description": "A simple Firefox extension",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs"
  ]
}
